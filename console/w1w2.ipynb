   "source": [
    "import numpy as np\n",
    "A = np.array([[0,1], [1,0], [1,1]])\n",
    "B = np.array([0,0,1])\n",
    "AT = A.T\n",
    "K = np.matrix(AT.dot(A))\n",
    "W = AT.dot(B)\n",
    "X = K.I.dot(W)\n",
    "print(Slice(X.tolist()))"
   ]
